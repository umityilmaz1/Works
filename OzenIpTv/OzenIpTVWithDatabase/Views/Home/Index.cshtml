    <script>

        $(document).ready(function () {

            //FILL CATEGORY LIST DROPDOWN
            $.ajax({
                url: "/Home/GetCategoryList",
                success: function (data) {
                    $.each(data, function (index, value) {
                        $("#selectCategory").append('<option value="' + value.ID + '">' + value.Name + '</option>');
                    });
                }
            });

            //SELECT CHANNELS FROM DROPDOWN BY CATEGORY
            $("#selectCategory").change(function () {
                $.ajax({
                    url: "/Home/GetChannelList",
                    data: {
                        "channelName": $("#searchText").val(),
                        "categoryID": $("#selectCategory").val()
                    },
                    contentType: "json",
                    success: function (data) {
                        $(".channelList tbody").html("");

                        $.each(data, function (index, value) {
                            $(".channelList tbody").append('<tr><td>' + value.Name + '</td><td>' + value.CategoryName + '</td></tr>');
                        });
                    }
                });
            });

            $("#searchText").keyup(function () {
                $.ajax({
                    url: "/Home/GetChannelList",
                    data: {
                        "channelName": $("#searchText").val(),
                        "categoryID": $("#selectCategory").val()
                    },
                    contentType: "json",
                    success: function (data) {
                        $(".channelList tbody").html("");

                        $.each(data, function (index, value) {
                            $(".channelList tbody").append('<tr><td>' + value.Name + '</td><td>' + value.CategoryName + '</td></tr>');
                        });
                    }
                });
            });
        });
    </script>

